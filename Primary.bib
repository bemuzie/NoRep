  author = {Brown, Matthew S and Pais, Richard and Qing, Peiyuan and Shah, Sumit and McNitt-Gray, Michael F and Goldin, Jonathan G and Petkovska, Iva and Tran, Lien and Aberle, Denise R},
  date = {2007},
  pages = {25--31},
  keywords = {“Computer-Aided Diagnosis”,“CT”,Journal Article,LN_volumetry_YES,“Lung Nodules”,measure_var_effects_NO,vol_measure_acc_NO,vol_measure_rep_NO}
