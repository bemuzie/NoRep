  keywords = {auto_seg_var_YES,Humans,Image Processing; Computer-Assisted,Journal Article,LN_volumetry_YES,Lung,manual_seg_var_NO,measure_var_effects_NO,observer variation,Phantoms; imaging,Reproducibility of results,Tomography; X-Ray Computed,vol_measure_acc_YES,vol_measure_rep_YES}
