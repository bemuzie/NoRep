  langid = {['eng']},
  journaltitle = {Computers in biology and medicine},
  shortjournal = {Comput. Biol. Med.},
  author = {Shen, Shiwen and Bui, Alex A T and Cong, Jason and Hsu, William},
  date = {2015-02},
  pages = {139--49},
  keywords = {Chain code,Computer aided diagnosis,Humans,Journal Article,Juxtapleural nodule,LN_volumetry_YES,Lung,measure_var_effects_NO,Radiographic Image Interpretation; Computer-Assisted,Research Support; U.S. Gov't; Non-P.H.S.,solitary pulmonary nodule,Support vector machine,vol_measure_acc_NO,vol_measure_rep_NO}
