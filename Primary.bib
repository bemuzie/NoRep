  keywords = {Comparative Study,Evaluation Studies as Topic,Humans,Journal Article,LN_volumetry_YES,Lung neoplasms,measure_var_effects_YES,Models; Structural,Tomography; X-Ray Computed,vol_measure_acc_NO,vol_measure_rep_NO}
