  keywords = {Adult,Aged,Female,Humans,Journal Article,LN_volumetry_YES,Lung neoplasms,Male,measure_var_effects_YES,Middle Aged,Prospective Studies,Radiographic Image Interpretation; Computer-Assisted,Research Support; Non-U.S. Gov't,Software,Tomography; X-Ray Computed,vol_measure_acc_NO,vol_measure_rep_NO},
