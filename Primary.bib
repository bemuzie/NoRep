  keywords = {Anatomy; Cross-Sectional,Artificial Intelligence,Humans,Imaging; Three-Dimensional,Information Storage and Retrieval,Journal Article,LN_volumetry_YES,Lung neoplasms,measure_var_effects_YES,Pattern Recognition; Automated,Radiographic Image Enhancement,Radiographic Image Interpretation; Computer-Assisted,Reproducibility of results,Sensitivity and Specificity,Tomography; X-Ray Computed,vol_measure_acc_NO,vol_measure_rep_NO},
  file = {ajr%2E05%2E1063.pdf:C\:\\Users\\КТ2\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\s2660lwu.default\\zotero\\storage\\HIKWUK9D\\ajr%2E05%2E1063.pdf:application/pdf}
