  date = {2010-04},
  pages = {403--12},
  keywords = {Comparative Study,Humans,Imaging; Three-Dimensional,Journal Article,LN_volumetry_YES,Lung neoplasms,measure_var_effects_NO,Pattern Recognition; Automated,Radiographic Image Interpretation; Computer-Assisted,Software,Tomography; X-Ray Computed,vol_measure_acc_NO,vol_measure_rep_NO}
