  author = {Kalpathy-Cramer, Jayashree and Zhao, Binsheng and Goldgof, Dmitry and Gu, Yuhua and Wang, Xingwei and Yang, Hao and Tan, Yongqiang and Gillies, Robert and Napel, Sandy},
  date = {2016-08},
  pages = {476--87},
  keywords = {auto_seg_var_YES,Infrastructure,Journal Article,LN_volumetry_YES,Lung cancer,manual_seg_var_NO,measure_var_effects_NO,Quantitative imaging,vol_measure_acc_YES,vol_measure_rep_YES}
