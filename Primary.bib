  date = {2015-06},
  pages = {373--9},
  keywords = {Computer Simulation,Humans,Imaging; Three-Dimensional,Journal Article,LN_volumetry_YES,Lung neoplasms,measure_var_effects_YES,Radiographic Image Interpretation; Computer-Assisted,Reproducibility of results,Sensitivity and Specificity,Tomography; X-Ray Computed,vol_measure_acc_NO,vol_measure_rep_NO},
  file = {10278_2014_Article_9753.pdf:C\:\\Users\\КТ2\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\s2660lwu.default\\zotero\\storage\\A2I4SDTM\\10278_2014_Article_9753.pdf:application/pdf}
