  langid = {['eng']},
  number = {5},
  journaltitle = {AJR. American journal of roentgenology},
  shortjournal = {AJR Am J Roentgenol},
  author = {Kawel, Nadine and Seifert, Burkhardt and Luetolf, Marcus and Boehm, Thomas},
  date = {2009-05},
  pages = {1324--9},
  keywords = {Adolescent,Adult,Aged,Aged; 80 and over,Female,Humans,Iohexol,Journal Article,Lung neoplasms,Male,measure_var_effects_YES,Middle Aged,Prospective Studies,Radiographic Image Interpretation; Computer-Assisted,Sensitivity and Specificity,Statistics; Nonparametric,Tomography; X-Ray Computed}
