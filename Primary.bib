  date = {2010-08},
  pages = {1878--85},
  keywords = {Aged,Comparative Study,Evaluation Studies,Female,Humans,Image Interpretation; Computer-Assisted,Journal Article,LN_volumetry_YES,Lung neoplasms,Male,measure_var_effects_YES,Middle Aged,Reproducibility of results,Sensitivity and Specificity,Software,Software Validation,solitary pulmonary nodule,Tomography; X-Ray Computed,vol_measure_acc_NO,vol_measure_rep_NO},
