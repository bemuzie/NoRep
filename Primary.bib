  file = {10278_2008_Article_9157.pdf:C\:\\Users\\КТ2\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\s2660lwu.default\\zotero\\storage\\K2JK99Q6\\10278_2008_Article_9157.pdf:application/pdf}
