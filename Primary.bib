  date = {2013-11},
  pages = {1364--70},
  keywords = {auto_seg_var_YES,detectable change,Humans,Journal Article,LN_volumetry_YES,manual_seg_var_NO,measure_var_effects_YES,nodule,Phantoms; imaging,phantom study,Radiographic Image Interpretation; Computer-Assisted,Research Support; Non-U.S. Gov't,ROC Curve,solitary pulmonary nodule,Tomography; X-Ray Computed,treatment response,vol_measure_acc_YES,vol_measure_rep_YES,Volumetric computed tomography}
