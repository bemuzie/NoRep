  shortjournal = {J Thorac Oncol},
  author = {Lee, Kyung Hee and Goo, Jin Mo and Park, Sang Joon and Wi, Jae Yeon and Chung, Doo Hyun and Go, Heounjeong and Park, Heae Surng and Park, Chang Min and Lee, Sang Min},
  date = {2014-01},
  pages = {74--82},
  keywords = {Adenocarcinoma,Adult,Aged,Aged; 80 and over,Female,Humans,Journal Article,LN_volumetry_YES,Lung neoplasms,Male,measure_var_effects_NO,Middle Aged,Retrospective Studies,Tomography; X-Ray Computed,vol_measure_acc_NO,vol_measure_rep_NO}
