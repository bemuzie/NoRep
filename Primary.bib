  date = {2009-01},
  pages = {24--31},
  keywords = {Carcinoma; Renal Cell,Comparative Study,Computer Graphics,Cone-Beam Computed Tomography,Humans,Image Processing; Computer-Assisted,Journal Article,Kidney Neoplasms,LN_volumetry_YES,Lung,Lung neoplasms,measure_var_effects_YES,Neoplasm Staging,observer variation,Retrospective Studies,Sarcoma,Sensitivity and Specificity,Soft Tissue Neoplasms,Software,Thyroid Neoplasms,Tomography; Spiral Computed,vol_measure_acc_NO,vol_measure_rep_NO}
