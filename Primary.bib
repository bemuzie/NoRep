
@article{checkley_investigation_1984,
  title = {An investigation into the problems of attenuation and area measurements made from CT images of pulmonary nodules.},
  volume = {8},
  issn = {0363-8715},
  abstract = {Pulmonary nodules were modelled with Perspex rods of various diameters. Computed tomographic (CT) images of these rods were obtained in the centre of the scan field, both in air and in a water bath. The numerical values at the centres of the rods were compared with values obtained from scan simulations that isolated the effects of the filtered backprojection. Both real scan and simulated data were reconstructed with different algorithms. Air was found to give markedly depressed attenuation values in real scans, but not in the simulations. This effect was considered to be the result of smoothing or inappropriate beam hardening corrections. In the simulated data, air resulted in elevated attenuation values due to overshoot of the point spread function. Such overshoots varied with object size and had a more extensive influence than could be appreciated from the real scan data. Different algorithms produced different central attenuation values as a result of differing overshoot effects. These and other artifact influences make attenuation measurements taken from CT images unreliable for the assessment of pulmonary nodules. Area measurements made over a period of time may more reliably discriminate between benign and malignant nodules.},
  timestamp = {2016-10-05T13:46:51Z},
  eprinttype = {pubmed},
  eprint = {6707272},
  langid = {['eng']},
  number = {2},
  journaltitle = {Journal of computer assisted tomography},
  shortjournal = {J Comput Assist Tomogr},
  author = {Checkley, D R and Zhu, X P and Antoun, N and Chen, S Z and Isherwood, I},
  date = {1984-04},
  pages = {237--43},
  keywords = {Diagnosis; Differential,Humans,Journal Article,LN_volumetry_NO,Lung,Lung Diseases,Lung neoplasms,Mathematics,measure_var_effects_NO,Models; Biological,Tomography; X-Ray Computed}
}

@article{kawel_effect_2009,
  title = {Effect of slab thickness on the CT detection of pulmonary nodules: use of sliding thin-slab maximum intensity projection and volume rendering.},
  volume = {192},
  issn = {1546-3141},
  doi = {10.2214/AJR.08.1689},
  abstract = {The objective of this study was to evaluate the detection rates of pulmonary nodules on CT as a function of slab thickness using sliding thin-slab maximum intensity projection (MIP) and volume rendering (VR).
Eighty-eight oncology patients (33 women, 55 men; mean age, 59 years; age range, 18-81 years) who routinely underwent chest CT examinations were prospectively included. Two radiologists independently evaluated each CT examination for the presence of pulmonary nodules using MIP and VR, with each image reconstructed using three different slab thicknesses (5, 8, 11 mm). The standard of reference was the maximum number of detected nodules, which were classified by localization and size, judged to be true-positives by a consensus panel. Interreader agreement was assessed by kappa value on a nodule-by-nodule basis. Sensitivities for both reconstruction techniques and for the three slab thicknesses were calculated using the proportion procedure for survey data with the patient as the primary sample unit and were compared using the Wilcoxon's signed rank test with Bonferroni correction for both readers separately.
One thousand fifty-eight true-positive nodules were detected. Interreader agreement was fair to moderate. Sensitivity for pulmonary nodules was superior for 8-mm MIP (reader 1, 84\%; reader 2, 81\%) and was significantly better than the sensitivities of all other tested techniques for both readers (p $<$ 0.001 each) independent of nodule localization and size (except for one reader's analysis of 8-mm MIP versus 11-mm MIP for nodules $>$ 8 mm). A higher sensitivity was achieved using MIP than VR.
MIP with a slab thickness of 8 mm is superior in the detection of pulmonary nodules to all other tested techniques.},
