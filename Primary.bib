  number = {9},
  journaltitle = {Academic radiology},
  shortjournal = {Acad Radiol},
  author = {Kimme-Smith, C and Hart, E M and Goldin, J G and Johnson, T D and Terwilliger, R and Aberle, D R},
  date = {1996-09},
  pages = {735--41},
  keywords = {Adult,Artifacts,Body Constitution,Diaphragm,Equipment Design,Heart,Humans,Journal Article,LN_volumetry_YES,measure_var_effects_YES,Methylmethacrylate,Methylmethacrylates,observer variation,Optics and Photonics,Phantoms; imaging,Radiographic Image Enhancement,Reproducibility of results,Research Support; Non-U.S. Gov't,Sensitivity and Specificity,Thorax,Tomography; X-Ray Computed}
