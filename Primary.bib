  shortjournal = {Br J Radiol},
  author = {Doo, K W and Kang, E-Y and Yong, H S and Woo, O H and Lee, K Y and Oh, Y-W},
  date = {2014-09},
  pages = {20130644},
  keywords = {Humans,Image Processing; Computer-Assisted,Journal Article,LN_volumetry_YES,measure_var_effects_NO,Organ Size,Phantoms; imaging,Radiographic Image Interpretation; Computer-Assisted,Randomized Controlled Trial,Research Support; Non-U.S. Gov't,solitary pulmonary nodule,Tomography; X-Ray Computed,vol_measure_acc_YES,vol_measure_rep_NO}
