  date = {2007-05},
  pages = {561--9},
  keywords = {Comparative Study,Female,Humans,Imaging; Three-Dimensional,Journal Article,LN_volumetry_NO,Lung neoplasms,Male,measure_var_effects_NO,Middle Aged,observer variation,Radiographic Image Enhancement,Radiographic Image Interpretation; Computer-Assisted,Reproducibility of results,Research Support; Non-U.S. Gov't,Sensitivity and Specificity,Tomography; X-Ray Computed,vol_measure_acc_NO,vol_measure_rep_NO}
